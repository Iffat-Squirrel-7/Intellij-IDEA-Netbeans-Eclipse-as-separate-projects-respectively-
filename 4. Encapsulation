class Account {
    private double balance;

    public void setBalance(double b) {
        if (b > 0)
            balance = b;
    }

    public double getBalance() {
        return balance;
    }
}

public class Main {
    public static void main(String[] args) {
        Account acc = new Account();
        acc.setBalance(3000);
        System.out.println(acc.getBalance());
    }
}
