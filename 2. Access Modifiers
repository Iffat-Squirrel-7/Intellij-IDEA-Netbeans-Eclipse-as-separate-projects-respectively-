class Demo {
    public int a = 10;
    private int b = 20;

    void show() {
        System.out.println(a);
        System.out.println(b);
    }
}

public class Main {
    public static void main(String[] args) {
        Demo d = new Demo();
        System.out.println(d.a);
        d.show();
    }
}
